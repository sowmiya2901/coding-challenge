  for(int i = 1; i<=n; i++){
             int found = 0;
             for(int j = 0; j<n-1; j++){
                 if(arr[j] == i){
                     found = 1;
                 }
             }
             
             if(found == 0){
                 return i;
             }
         }
         
         return 0;
